# AWS SES Configuration
# Replace these with your actual AWS credentials and settings

# AWS Region where your SES service is configured
VITE_AWS_REGION=us-east-1

# AWS Access Key ID with SES permissions
VITE_AWS_ACCESS_KEY_ID=your_access_key_id_here

# AWS Secret Access Key
VITE_AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# Email address to send FROM (must be verified in SES)
VITE_SES_FROM_EMAIL=noreply@rulerev.com

# Email address to send TO (where you want to receive messages)
VITE_SES_TO_EMAIL=hello@rulerev.com